page{
    height:100%;
    overflow: hidden;
}

.swiperContainer{
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    z-index:2;
}

.mask{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgb(0,0,0);
    opacity: 0;
    z-index: 2;
}
.swiper{
    height: 100%;
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    z-index:3;
    transform: scale(0);
}
.swiper-item{
    position: relative;
    display: flex;
    justify-content: center;
}
.swiper-item-image{
    display: flex;
    justify-content: center;
    height: 922rpx;
    width: 560rpx;
    text-align: center;
    padding: 0px 0rpx;
    transform: scale(.9);
    box-shadow: 1px 2px 16px 0px rgba(0, 0, 0, 0.14);
    background:rgba(0,0,0,.06);
    margin-top:134rpx;
    transition: all .5s ease-out;
}

.X-margin-top-240{
    margin-top: 240rpx;
}

.selected {
    transform: scale(1);
}

.swiper-image{
    width:560rpx;
    height:922rpx;
}

.button {
    left: 50%;
    transform: translateX(-50%);
    position: fixed;
    text-align: center;
    font-size: 36rpx;
    color: #fff;
    width: 560rpx;
    height: 88rpx;
    line-height: 88rpx;
    background-color: #46d58d;
    bottom: 144rpx;
    border-radius: 8rpx;
    z-index: 3;
}
.close-button {
    position: fixed;
    width: 80rpx;
    height: 80rpx;
    bottom: 32rpx;
    left: 50%;
    transform: translateX(-50%);
    z-index: 3;
}
.ad-ticket {
    background: #fff;
    box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.1);
    position: relative;
    -webkit-mask: url(data:image/png;base64,) repeat-x 2rpx 0/53rpx 15rpx, linear-gradient(0deg, rgba(0,0,0,1) 0, rgba(0,0,0,1) 100%) no-repeat 0 14rpx/650rpx 892rpx, url(data:image/png;base64,) repeat-x 22rpx bottom/53rpx 18rpx;
    -webkit-mask-composite: add;
}

.showChoice {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    background: #000;
    color: #09ff00;
    position: absolute;
    bottom: 0px;
    left: 0;
    z-index: 1;
}

.dsn {
    display: none;
}

